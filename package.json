{
  "name": "express-sample",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "rimraf dist && set DEBUG=express_template:* & tsc-watch --noClear --onSuccess \"node ./bin/www\"",
    "start": "rimraf dist && tsc && export NODE_ENV=production && node ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "log4js": "^6.2.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.4",
    "rimraf": "^3.0.2",
    "tsc-watch": "^4.1.0",
    "typescript": "^3.8.2"
  }
}
